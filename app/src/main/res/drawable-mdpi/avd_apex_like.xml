<animated-vector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    tools:targetApi="lollipop">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="98dp"
            android:height="85dp"
            android:viewportWidth="98"
            android:viewportHeight="85">
            <group
                android:name="group"
                android:pivotX="50"
                android:pivotY="50">
                <path
                    android:name="path"
                    android:pathData="M 49.14 18.768 C 36.413 -11.012 0.239 -4.064 0.001 30.5 C -0.131 49.483 17.226 56.579 28.784 64.167 C 39.99 71.528 47.966 81.595 49.216 85.881 C 50.286 81.681 59.169 71.329 69.543 63.962 C 80.886 55.906 98.458 49.276 98.325 30.293 C 98.085 -4.356 61.282 -9.825 49.14 18.768 Z"
                    android:fillColor="#d40000"
                    android:strokeWidth="1"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="0.85"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="0.85"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="150"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
